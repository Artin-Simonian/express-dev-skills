<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
 
  <h3>Skills as of <%= time %></h3>
  <ul>
  
    <% skills.forEach(function(t) { %>
      <li style="list-style: none;">
        <form action="/skills/<%= t.id %>?_method=DELETE"
          class="delete-form" method="POST">
          <button type="submit">X</button>
        </form>
        <a style="text-decoration: none;" href="/skills/<%= t.id %>"><%= t.skill %></a>
      </li>
    <% }); %>
  </ul>
  <a href="/skills/new">Add To-Do</a>
  
</body>
</html>